function yourFunction(){
    let params = `scrollbars=no,resizable=no,status=no,location=no,t     oolbar=no,menubar=no, width=600,height=300,left=100,top=100`;

    open('https://anikapuri.github.io/4Vision/', 'test', params);
		//open(url you want to open, name, popup window features)
    
    setTimeout(yourFunction, 1000);
    //makes sure the data is always updated 
    
    
}

